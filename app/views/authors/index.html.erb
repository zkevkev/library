<nav>
  <%= link_to "Books", "/child_table_name" %>
</nav>

<h1>Authors</h1>

<%= link_to "New Author", "/parents/new" %>

<% @authors.each do |author| %>
  <h3>
    <%= link_to "#{author.name}", "/parents/#{author.id}" %> created: <%= author.created_at %></br>
    <%= link_to "Update #{author.name}", "/parents/#{author.id}/edit" %>
  

  <%= form_with url: "/parents/#{author.id}", method: :delete, local: true do |form| %>
    <%= form.submit "Delete #{author.name}" %>
  <% end %>
  </h3>
<% end %>